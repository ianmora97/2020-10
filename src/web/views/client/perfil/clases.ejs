<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="mobile-web-app-capable" content="yes">
    <link rel="shortcut icon" type="image/x-icon" href="../../img/favicon.svg">
    <title>Clases</title>
    <link rel="stylesheet" href="../css/bootstrap.css">
    <link rel="stylesheet" href="../css/calendar-horario.css">
    <link rel="stylesheet" href="../css/general.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/all.min.css" />
    <link rel="stylesheet" href="../../css/animate.css">
    <script src="../js/jq-bs-bundle.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.22.2/moment.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/fullcalendar/3.9.0/fullcalendar.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/air-datepicker/2.2.3/js/datepicker.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/air-datepicker/2.2.3/js/i18n/datepicker.en.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2/dist/Chart.min.js"></script>
</head>

<body class="bg-white">
    <%- include('perfilHeader') %>
    <div class="container-fluid w-90 mt-3">
        <div class="bg-light p-2 shadow-sm rounded-lg">
            <ul class="nav nav-tabs nav-clases" id="myTab" role="tablist">
                <li class="nav-item" role="presentation">
                    <a class="nav-link active" id="matricula-activa-tab" data-toggle="tab" href="#matricula-activa" role="tab" aria-controls="matricula-activa" aria-selected="true">Matricula Activa</a>
                </li>
                <li class="nav-item" role="presentation">
                    <a class="nav-link" id="historial-matricula-tab" data-toggle="tab" href="#historial-matricula" role="tab" aria-controls="historial-matricula" aria-selected="false">Historial de Cursos</a>
                </li>
            </ul>
            <div class="tab-content" id="myTabContent">
                <div class="tab-pane fade show active bg-white" id="matricula-activa" role="tabpanel" aria-labelledby="matricula-activa-tab" style="height: 400px;">
    
                </div>
                <div class="tab-pane fade  bg-white" id="historial-matricula" role="tabpanel" aria-labelledby="historial-matricula-tab" style="height: 400px;">
    
                </div>
            </div>
        </div>
        <div class="bg-light p-2 shadow-sm rounded-lg mt-4">
            <div class="px-3 py-2">
                <div class="bg-white px-2 py-1 rounded-lg mb-3">
                    <h2 class="my-2">Horario</h2>
                </div>
                <div class="card">
                    <div class="card-body p-0">
                        <div id="calendar"></div>
                    </div>
                </div>
              </div>
        </div>
    </div>
    <%- include('perfilFooter') %>
    <script src="../js/client/perfil/calendario-horario.js"></script>
</body>

</html>