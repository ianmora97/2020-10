                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_dislipidemia_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_dislipidemia_info_medica-1"
                                                        class="form-control w-100" style="border-style: none;"></td>
                                            </tr>
                                            <tr>
                                                <td>Epilepsia o convulsiones</td>

                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_convulciones_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_convulciones_info_medica-1"
                                                        class="form-control w-100" style="border-style: none;"></td>
                                            </tr>
                                            <tr>
                                                <td>Fracturas</td>

                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_fracturas_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_fracturas_info_medica-1"
                                                        class="form-control w-100" style="border-style: none;"></td>
                                            </tr>
                                            <tr>
                                                <td>Cirugías</td>

                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_cirujias_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_cirujias_info_medica-1"
                                                        class="form-control w-100" style="border-style: none;"></td>
                                            </tr>
                                            <tr>
                                                <td>Hernias</td>

                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_hernias_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_hernias_info_medica-1"
                                                        class="form-control w-100" style="border-style: none;"></td>
