<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="mobile-web-app-capable" content="yes">
    <link rel="shortcut icon" type="image/x-icon" href="../../img/favicon.svg">
    <title>Padecimientos</title>
    <link rel="stylesheet" href="../css/bootstrap.css">
    <link rel="stylesheet" href="../css/general.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.14.0/css/all.min.css" />
    <link rel="stylesheet" href="../../css/animate.css">
    <script src="../js/jq-bs-bundle.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/fullcalendar@5.3.0/main.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2/dist/Chart.min.js"></script>
</head>

<body>
    <%- include('perfilHeader') %>
    <div class="container mt-3">
        <div class="card shadow mb-5">
            <div class="card-header text-center py-3">
                <div>
                    <div class="" id="collapse-1">
                        <div class="card">
                            <div class="card-body">
                                <div class="row no-gutters">
                                    <div class="col-md-4 px-3">
                                        <label for="first_name">
                                        <strong>En caso de
                                                emergencia avisar a:</strong><br></label>
                                        <div class="form-group"><input type="text" id="nombre_emergencia_medica-1"
                                                class="form-control"></div>
                                    </div>
                                    <div class="col-md-4 px-3"><label
                                            for="first_name"><strong>Celular&nbsp;&nbsp;</strong><br></label>
                                        <div class="form-group"><input type="text" id="celular_emergencia_medica-1"
                                                class="form-control">
                                        </div>
                                    </div>
                                    <div class="col-md-4 px-3"><label for="first_name"><strong>Parentesco</strong><br></label>
                                        <div class="form-group"><input type="text" id="parentesco_emergencia_medica-1"
                                                class="form-control"></div>
                                    </div>
                                </div>
                                <div class="table-responsive text-left table mt-2" id="dataTable-2" role="grid"
                                    aria-describedby="dataTable_info">
                                    <table class="table my-0" id="dataTable">
                                        <thead>
                                            <tr>
                                                <th><strong>Enfermedad, lesión o padecimiento</strong> </th>
                                                <th><strong>Check</strong></th>
                                                <th><strong>Observaciones</strong></th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            <tr>
                                                <td class="text-left">Tabaquismo</td>
                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_alcohol_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_alcohol_info_medica-1"
                                                        class="form-control w-100" style="border-style: none;"></td>
                                            </tr>
                                            <tr>
                                                <td class="text-left">Alcoholismo</td>
                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_alcohol_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_alcohol_info_medica-1"
                                                        class="form-control w-100" style="border-style: none;"></td>
                                            </tr>
                                            <tr>
                                                <td class="text-left">Sobrepeso u Obesidad</td>

                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_obesidad_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_obesidad_info_medica-1"
                                                        class="form-control w-100" style="border-style: none;"></td>
                                            </tr>
                                            <tr>
                                                <td>Sedentarismo</td>

                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_sedentarismo_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_sedentarismo_info_medica-1"
                                                        class="form-control w-100" style="border-style: none;"></td>
                                            </tr>
                                            <tr>
                                                <td>Enfermedades del corazón</td>

                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_enfermedades_corazon_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_enfermedades_corazon_info_medica-1"
                                                        class="form-control w-100" style="border-style: none;"></td>
                                            </tr>
                                            <tr>
                                                <td>Lesiones articulares</td>

                                                <td>
                                                    <div class="custom-control custom-switch">
                                                        <input type="checkbox" class="custom-control-input"
                                                            id="check_lesiones_articulares_info_medica">
                                                        <label class="custom-control-label" for="customSwitch1"></label>
                                                    </div>
                                                </td>
                                                <td><input type="text" id="text_lesiones_articulares_info_medica-1"
